{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-trailing-spaces": "off",
        "linebreak-style": "off",
        "eol-last": "off",
        "indent": "off",
        "space-before-blocks":"off",
        "max-len":"off",
        "import/newline-after-import": "off"
    }
}
